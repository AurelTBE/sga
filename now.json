{
  "version": 2,
  "routes": [
    {
      "src": "^/firebase-messaging-sw.js$",
      "dest": "/_next/static/firebase-messaging-sw.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "env": {
    "FCM_SERVER_KEY": "@fcm-auth"
  },
  "build": {
    "env": {
      "FCM_SERVER_KEY": "@fcm-auth"
    }
  }
}